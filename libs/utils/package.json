{
  "name": "@ph/utils",
  "private": true,
  "version": "0.0.0",
  "module": "./src/index.ts",
  "main": "./src/index.ts",
  "type": "module",
  "scripts": {
    "lint": "biome lint ./src",
    "check": "biome check ./src",
    "check:apply-safe": "biome check --apply ./src",
    "check:apply-unsafe": "biome check --apply-unsafe ./src",
    "format": "biome format ./src",
    "format:write": "biome format --write ./src",
    "typecheck": "tsc -p ./tsconfig.json"
  }
}
